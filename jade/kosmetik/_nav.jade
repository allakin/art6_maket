-var menu = {'Главная': 'main.html', 'Каталог продукции': 'catalog.html', 'Карточка товара': 'card.html', 'Вход в личный кабинет': 'login.html', 'Личный кабинет': 'user.html', 'Доставка': 'pocket.html'};

nav.navbar.navbar-default
 .container-fluid
   ul.nav.navbar-nav
      each val, key in menu
        if selected === key
          li.active
            a(href=val, title=key)= key
        else
          li
            a(href=val, title=key)= key
