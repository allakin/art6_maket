-var menu = {'Главная': 'main.html', 'Категория услуг': 'inside-page.html', 'Карточка услуг': 'card.html', 'Новости': 'news.html'};

nav.navbar.navbar-default
 .container-fluid
   ul.nav.navbar-nav
      each val, key in menu
        if selected === key
          li.active
            a(href=val, title=key)= key
        else
          li
            a(href=val, title=key)= key
